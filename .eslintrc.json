{
  "root": true,
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "standard",
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "angular": true
  },
  "rules": {
    "generator-star-spacing": "error",
    "no-debugger": "error",
    "no-new": "off"
  }
}
